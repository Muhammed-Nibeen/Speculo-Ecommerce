    <script>    

          // Add event listener to the trash icon link with a class "delete-product"
          document.querySelectorAll('.delete-product').forEach(item => {
            console.log("enternsdflksdfjdfas");
            item.addEventListener('click', function(event) {
              event.preventDefault(); // Prevent default link behavior
              const productId = this.getAttribute('data-product-id'); // Get product ID

              // Open the custom modal when the trash icon is clicked
              $('#confirmDeleteModal').modal('show');

              // Handle confirmation button click
              document.getElementById('confirmDeleteBtn').addEventListener('click', function() {
                // Redirect or perform deletion action here using productId
                window.location.href = `/removefromcart/${productId}`; // Redirect to the delete endpoint
              });
            });
          });

          function message(message){
            const limitmessage = document.getElementById('message')
            limitmessage.innerHTML = message;
          }

          function subtractQuantity(productId, price, index) {
            fetch(`/subcartquantity/${productId}`)
              .then(response => {
                return response.json();
              })
              .then(data => {
                updateQuantityAndTotal(data.updatedQuantity, price, index);
                substractQuantityUpdateTotal(price)
              })
              .catch(error => {
                console.error('Error:', error);
              });
          }

            function addQuantity(productId, price, index) {
              fetch(`/addcartquantity/${productId}`)
                .then(response => {
                  return response.json();
                })
                .then(data => {
                  if(data.updatedQuantity !== undefined){
                    updateQuantityAndTotal(data.updatedQuantity, price, index);
                    addQuantityUpdateTotal(price)
                  }else if(data.message !== undefined){
                    message(data.message)
                  }
                  
                })
                .catch(error => {
                  console.error('Error:', error);
                });
              }
              function updateQuantityAndTotal(quantity, price, index) {
                console.log("updaing");
                const limitmessage = document.getElementById('message')
                limitmessage.innerHTML = ''

                const quantityInput = document.querySelector(`.quantityInput_${index}`);
                quantityInput.value = quantity;


              const totalPrice = document.querySelector(`.total_${index}`);
              totalPrice.innerHTML = `$ ${price * quantity}`;
              }

              function substractQuantityUpdateTotal(price) {
              console.log("substracing")
              const subtotalText = document.getElementById('subtotal').innerText;
              console.log("subtotalText: ",subtotalText)
              const subtotal = parseInt(subtotalText)
              const priceint = parseInt(price)
              const newsubtotal = subtotal - priceint
              document.getElementById('subtotal').innerHTML = newsubtotal
              }
              
             

              function addQuantityUpdateTotal(price) {
              console.log('adding')
              const subtotalText = document.getElementById('subtotal').innerText;
              console.log("subtotalText: ",subtotalText)
              const subtotal = parseInt(subtotalText)
              const priceint = parseInt(price)
              const newsubtotal = subtotal + priceint
              document.getElementById('subtotal').innerHTML = newsubtotal
              }
    </script>

